create database if not exists crunch;

use crunch;

create table if not exists User1
(
ID char(277) not null,
Name varchar(10) not null,
Industry varchar(10) not null,
Description varchar(10),
TotalCash_inflow char(5),
TotalCash_outflow char(5),
PRIMARY KEY (Name, ID);
)


create table if not exists Project_Duration 
(
Project_ID varchar(10) not null, 
Start DATETIME DEFAULT (getdate()),
End DATETIME DEFAULT (getdate()),
FOREIGN KEY ( Project_ID ) REFERENCES User1(ID) ON DELETE CASCADE
);
